{
  "name": "starbucks-mobile",
  "version": "1.0.0",
  "scripts": {
    "start": "npx expo start --clear",
    "android": "npx expo run:android",
    "srn": "npx react-native start",
    "srn-a": "npx react-native run-android",
    "ios": "npx expo run:ios",
    "ios-user": "npx expo run:ios --device 'Zahniâ€™s iPhone'",
    "web": "npx expo start --web",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "platforms": "npx expo prebuild",
    "ios-build": "eas build --profile development --platform ios",
    "open-xcode": "xed ios",
    "platform-safe": "npx expo prebuild -p ios",
